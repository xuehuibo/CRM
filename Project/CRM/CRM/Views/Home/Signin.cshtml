@{
    ViewBag.Title = "用户登录";
}
@Styles.Render("~/bundles/signcss")
<div class="container">
    <form class="form-signin" role="form">
        <h2 class="form-signin-heading">请登录</h2>
        <input class="form-control" type="text" autofocus="" required="" placeholder="用户名"/>
        <input class="form-control" type="password" autofocus="" required="" placeholder="密码"/>
        <div class="checkbox">
            <label>
                <input type="checkbox" />  保持登录
            </label>
            
        </div>
        <a class="btn btn-primary btn-lg btn-block">登录</a>
    </form>
</div>